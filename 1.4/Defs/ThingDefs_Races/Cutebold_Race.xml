			<baseHungerRate>0.65</baseHungerRate>
			<foodType>OmnivoreHuman</foodType>
			<gestationPeriodDays>15</gestationPeriodDays>
			<manhunterOnDamageChance>0.20</manhunterOnDamageChance>
			<manhunterOnTameFailChance>0.02</manhunterOnTameFailChance>
			<litterSizeCurve>
				<points>
					<li>(0.5, 0)</li>
					<li>(1, 1)</li>
					<li>(1.01, 0.02)</li>
					<li>(3.5, 0)</li>
				</points>
			</litterSizeCurve>
			<lifeStageAges>
				<li Class="AlienRace.LifeStageAgeAlien">
					<def>Cutebold_Baby</def>
					<minAge>0</minAge>
					<headOffsetDirectional>
						<north>(0,0.05)</north>
						<east>(0.01,0.05)</east>
						<west>(-0.01,0.05)</west>
					</headOffsetDirectional>
				</li>
				<li Class="AlienRace.LifeStageAgeAlien">
					<def>Cutebold_Child</def>
					<minAge>2</minAge>
					<headOffsetDirectional>
						<east>(0.04,0)</east>
						<west>(-0.04,0)</west>
					</headOffsetDirectional>
				</li>
				<li MayRequire="Ludeon.RimWorld.Biotech"  Class="AlienRace.LifeStageAgeAlien">
					<def>Cutebold_PreTeenager</def>
					<minAge>5</minAge>
					<headOffsetDirectional>
						<south>(0,-0.01)</south>
						<north>(0,-0.01)</north>
						<east>(0.05,-0.01)</east>
						<west>(-0.05,-0.01)</west>
					</headOffsetDirectional>
				</li>
				<li Class="AlienRace.LifeStageAgeAlien">
					<def>Cutebold_Teenager</def>
					<minAge>9</minAge>
				</li>
				<li Class="AlienRace.LifeStageAgeAlien">
					<def>Cutebold_Adult</def>
					<minAge>12</minAge>
