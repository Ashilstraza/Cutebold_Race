	<PawnKindDef ParentName="CuteboldCivBase">
		<defName>Cutebold_Outsider</defName>
		<label>outsider</label>
		<defaultFactionType>Ancients</defaultFactionType>
		<!--We do this to make these cutebolds not have a regular faction-->
		<combatPower>70</combatPower>
		<apparelAllowHeadgearChance>.1</apparelAllowHeadgearChance>
		<invNutrition>1.6575</invNutrition>
		<itemQuality>Normal</itemQuality>
		<apparelMoney>100~1500</apparelMoney>
		<apparelTags>
			<li>IndustrialBasic</li>
			<li>IndustrialAdvanced</li>
			<li>IndustrialMilitaryBasic</li>
			<li>BeltDefensePop</li>
		</apparelTags>
		<apparelRequired>
			<li MayRequire="CETeam.CombatExtended">CE_Apparel_Backpack</li>
		</apparelRequired>
		<backstoryCryptosleepCommonality>0.1</backstoryCryptosleepCommonality>
		<techHediffsMoney>50~500</techHediffsMoney>
		<techHediffsTags>
			<li>Poor</li>
			<li>Simple</li>
			<li>ImplantEmpireCommon</li>
		</techHediffsTags>
		<techHediffsChance>0.15</techHediffsChance>
		<weaponMoney>150~3000</weaponMoney>
		<weaponTags>
			<li>Neolithic</li>
			<li>Medieval</li>
			<li>SimpleGun</li>
		</weaponTags>
		<inventoryOptions>
			<skipChance>0.45</skipChance>
			<subOptionsChooseOne>
				<li>
					<choiceChance>90</choiceChance>
					<thingDef>Silver</thingDef>
					<countRange>2~60</countRange>
				</li>
				<li>
					<choiceChance>10</choiceChance>
					<thingDef>Silver</thingDef>
					<countRange>100~300</countRange>
				</li>
			</subOptionsChooseOne>
		</inventoryOptions>
		<gearHealthRange>0.2~1.2</gearHealthRange>
		<invFoodDef>Pemmican</invFoodDef>
		<backstoryFiltersOverride>
			<li>
				<categories>
					<li>Offworld</li>
					<li>Outlander</li>
					<li>Outsider</li>
					<li>Pirate</li>
					<li>Civil</li>
				</categories>
				<commonality>1.0</commonality>
			</li>
		</backstoryFiltersOverride>
		<modExtensions>
			<li MayRequire="CETeam.CombatExtended" Class="CombatExtended.LoadoutPropertiesExtension">
				<primaryMagazineCount>
					<min>3</min>
					<max>5</max>
				</primaryMagazineCount>
				<shieldMoney>
					<min>100</min>
					<max>1800</max>
				</shieldMoney>
				<shieldTags>
					<li>OutlanderShield</li>
				</shieldTags>
				<shieldChance>0.9</shieldChance>
				<sidearms>
					<li>
						<sidearmMoney>
							<min>50</min>
							<max>800</max>
						</sidearmMoney>
						<weaponTags>
							<li>CE_Sidearm_Melee</li>
						</weaponTags>
					</li>
				</sidearms>
			</li>
		</modExtensions>
	</PawnKindDef>
