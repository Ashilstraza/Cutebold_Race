<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- Cutebold Base -->
	<PawnKindDef Name="CuteboldBase" Abstract="True">
		<defaultFactionType>CuteboldTribe</defaultFactionType>
		<race>Alien_Cutebold</race>
		<isFighter>false</isFighter>
		<combatPower>20</combatPower>
		<chemicalAddictionChance>0.04</chemicalAddictionChance>
		<apparelAllowHeadgearChance>0</apparelAllowHeadgearChance>
		<gearHealthRange>0.2~1.1</gearHealthRange>
		<apparelMoney>50~125</apparelMoney>
		<apparelTags>
			<li>Neolithic</li>
		</apparelTags>
		<backstoryCryptosleepCommonality>0</backstoryCryptosleepCommonality>
		<techHediffsMoney>50~50</techHediffsMoney>
		<techHediffsTags>
			<li>Poor</li>
		</techHediffsTags>
		<techHediffsChance>0.075</techHediffsChance>
		<modExtensions/>
	</PawnKindDef>
	<!-- Cutebold Tribal -->
	<PawnKindDef ParentName="CuteboldBase">
		<defName>Cutebold</defName>
		<defaultFactionType>CuteboldTribePlayer</defaultFactionType>
		<label>member</label>
		<apparelIgnoreSeasons>true</apparelIgnoreSeasons>
		<forceNormalGearQuality>true</forceNormalGearQuality>
		<backstoryCategories>
			<li>CuteboldBackstory</li>
		</backstoryCategories>
	</PawnKindDef>
	<!-- Cutebold Civilian Base -->
	<PawnKindDef Name="CuteboldCivBase" ParentName="CuteboldBase" Abstract="True">
		<invNutrition>1.6575</invNutrition>
		<invFoodDef>Pemmican</invFoodDef>
		<itemQuality>Poor</itemQuality>
		<weaponMoney>50~100</weaponMoney>
		<weaponTags>
			<li>NeolithicMeleeBasic</li>
			<li>NeolithicRangedBasic</li>
		</weaponTags>
		<inventoryOptions>
			<skipChance>0.45</skipChance>
			<subOptionsChooseOne>
				<li>
					<choiceChance>90</choiceChance>
					<thingDef>Silver</thingDef>
					<countRange>2~20</countRange>
				</li>
				<li>
					<choiceChance>10</choiceChance>
					<thingDef>Silver</thingDef>
					<countRange>30~70</countRange>
				</li>
			</subOptionsChooseOne>
		</inventoryOptions>
		<modExtensions>
			<li MayRequire="CETeam.CombatExtended" Class="CombatExtended.LoadoutPropertiesExtension" Inherit="True">
				<primaryMagazineCount>
					<min>25</min>
					<max>50</max>
				</primaryMagazineCount>
			</li>
		</modExtensions>
	</PawnKindDef>
	<!-- Cutebold Civilians -->
