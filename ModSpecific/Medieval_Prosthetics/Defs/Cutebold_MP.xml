<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- Artificial Tail-->
	<HediffDef ParentName="addedPartMedArtificial">
		<defName>ArtificialCuteboldMedTail</defName>
		<label>artificial cutebold tail</label>
		<labelNoun>an artificial cutebold tail</labelNoun>
		<description>An installed floppy cutebold tail.</description>
		<descriptionHyperlinks>
			<ThingDef>ArtificialCuteboldMedTail</ThingDef>
		</descriptionHyperlinks>
		<spawnThingOnRemoved>ArtificialCuteboldMedTail</spawnThingOnRemoved>
		<addedPartProps>
			<solid>true</solid>
			<partEfficiency>0.75</partEfficiency>
			<betterThanNatural>false</betterThanNatural>
		</addedPartProps>
	</HediffDef>
	<ThingDef ParentName="ArtificialMed">
		<defName>ArtificialCuteboldMedTail</defName>
		<label>artificial cutebold tail</label>
		<description>A floppy cutebold tail. It is a little better than a simple fabric tail. Filled with trees and hard rocks.</description>
		<costList>
			<Steel>40</Steel>
			<WoodLog>5</WoodLog>
		</costList>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<costStuffCount>40</costStuffCount>
		<statBases>
			<Mass>1.25</Mass>
		</statBases>
	</ThingDef>
	<RecipeDef ParentName="SurgeryArtificialCuteboldTail">
		<defName>InstallArtificialCuteboldMedTail</defName>
		<label>install artificial cutebold tail</label>
		<description>Installs an artificial cutebold tail.</description>
		<descriptionHyperlinks>
			<ThingDef>ArtificialCuteboldMedTail</ThingDef>
		</descriptionHyperlinks>
		<jobString>Installing artificial tail.</jobString>
		<workAmount>350</workAmount>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>ArtificialCuteboldMedTail</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>ArtificialCuteboldMedTail</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>CuteboldTail</li>
		</appliedOnFixedBodyParts>
		<addsHediff>ArtificialCuteboldMedTail</addsHediff>
	</RecipeDef>
